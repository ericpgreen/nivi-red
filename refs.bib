  urldate = {2018-04-02},
  date = {2017-11},
  pages = {713--743},
  author = {Prata, Ndola and Fraser, Ashley and Huchko, Megan J. and Gipson, Jessica D. and Withers, Mellissa and Lewis, Shayna and Ciaraldi, Erica J. and Upadhyay, Ushma D.},
  file = {/Users/ericgreen/Zotero/storage/6W2AQ479/Prata et al. - 2017 - WOMEN‚ÄôS EMPOWERMENT AND FAMILY PLANNING A REVIEW .pdf;/Users/ericgreen/Zotero/storage/RMMGNFFL/DB0C0FC1BB23407AC9C256556DAE435B.html}
